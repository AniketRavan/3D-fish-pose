python runme_validate_parallel.py -e 1  -d "220919_ob_oldCal" -o "outputs/val_221001_2_ob_oldCal_best_lambda_5_val_221001" -p "proj_params_201711_corrected" -m "resnet_pose_221001_2_best_lambda_5.pt"
echo Finished ob_oldCal
python runme_validate_parallel.py -e 1  -d "220919_ob_newCal" -o "outputs/val_221001_2_ob_newCal_best_lambda_5_val_221001" -p "proj_params_101019_corrected_new" -m "resnet_pose_221001_2_best_lambda_5.pt"
